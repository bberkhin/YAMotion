{
  "syntax": true,
  "fold": true,
  "indent": true,
  "wrapMode": false,
  "displayEOL": false,
  "indentGuide": false,
  "lineNumber": true,
  "longLineOn": false,
  "whiteSpace": false,
  "tabWidth": 4,
  "visibleHSB": true,
  "enableLogExecution": false,
  "theme_syntax": "them_black.json",
  "postpocessing": "wincnc.con",
  "files": [
    {
      "name": "GCODE",
      "syntax": "nc_syntax_black.json",
      "extensions": "*.ngc;*.nc;*.cnc;*.tap"
    },
    {
      "name": "GCMC",
      "syntax": "gcmc_syntax_white.json",
      "extensions": "*.gcmc"
    },
    {
      "name": "JSON",
      "extensions": "*.json",
      "syntax": "json_syntax_black.json"
    },
    {
      "name": "ALL",
      "extensions": "*.*"
    }
  ]
}