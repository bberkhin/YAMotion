<?xml version="1.0" encoding="UTF-8"?>
<macroses>
  <macros>
    <name>Round Rect Pocket</name>
    <desc>Millng round rect pocket</desc>
    <image>pocket_roundrect.jpg</image>
    <gcmc>pocket_roundrect.gcmc</gcmc>
    <include>pocket_roundrect.inc.gcmc</include>
    <function>DoRoundRectPocket(xMin, yMin, xD, yD, radius, zSave, d, zDelta,zFull,per_stepover,rate,zRate,ccw)</function>
    <arguments>
      <argument defvalue ="2" type="scalar" ref="zSave">
        <name>Save Z:</name>
      </argument>
      <argument defvalue ="0" type="scalar" ref="xMin">
        <name>X Min:</name>
        <desc>X Min of the rect in mm</desc>
      </argument>
      <argument defvalue ="0" type="scalar" ref="yMin">
        <name>Y Min:</name>
        <desc>Y Min of the rect in mm</desc>
      </argument>
      <argument defvalue ="200" type="scalar" ref="xD">
        <name>Width (dX):</name>
        <desc>Width of the rect in mm</desc>
      </argument>
      <argument defvalue ="100" type="scalar" ref="yD">
        <name>Height (dY):</name>
        <desc>Height of the rect in mm</desc>
      </argument>
      <argument defvalue ="20" type="scalar" ref="radius">
        <name>Radius:</name>
        <desc>Round rect radius in mm</desc>
      </argument>
      <argument defvalue ="6" type="scalar" ref="d">
        <name>Mill diameter</name>
      </argument>
      <argument defvalue ="200" type="scalar" ref="rate">
        <name>Feed rate:</name>
        <desc>Feed rate of cutting</desc>
      </argument>
      <argument defvalue ="50" type="scalar" ref="zRate">
        <name>Z feed rate:</name>
        <desc>Z feed rate of cutting</desc>
      </argument>
      <argument defvalue ="30" type="scalar" ref="per_stepover">
        <name>Step over(%):</name>
      </argument>    
      <argument defvalue ="1" type="scalar" ref="zDelta">
        <name>Step Z:</name>
      </argument>
      <argument defvalue ="1" type="scalar" ref="zFull">
        <name>Thikness:</name>
      </argument>
      <argument defvalue ="1" type="list" ref="ccw">
        <name>Direction:</name>
        <desc>Direction: 0 - clockwise, 2- counter clockwise</desc>
        <list value ="0" name="Clockwise"/>
        <list value ="1" name="Counter Clockwise"/>
      </argument>
    </arguments>
  </macros>
</macroses>